obo:FOODON_03400757 rdf:type owl:Class ;
                    rdfs:subClassOf obo:FOODON_03400660 ;
                    obo:IAO_0000114 obo:IAO_0000428 ;
                    obo:IAO_0000115 """Dried vegetables. The commodities of this group are in general artificially dried and often comminuted. Exposure to pesticides is from pre-harvest applications and or treatment of the dry commodities.
The entire commodity may be consumed after soaking or boiling."""@en ;
                    obo:IAO_0000412 "http://langual.org" ;
                    oboInOwl:hasDbXref "http://www.langual.org/langual_thesaurus.asp?termid=A0757" ;
