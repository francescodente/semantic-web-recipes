###  http://purl.obolibrary.org/obo/FOODON_03400744
obo:FOODON_03400744 rdf:type owl:Class ;
                    rdfs:subClassOf obo:FOODON_03400655 ;
                    obo:IAO_0000114 obo:IAO_0000428 ;
                    obo:IAO_0000115 """Eggs are the fresh edible portion of the body produced by female birds, especially domestic fowl.
The edible portion includes egg yolk and egg white after removal of the shell."""@en ;
                    obo:IAO_0000412 "http://langual.org" ;
                    oboInOwl:hasDbXref "http://www.langual.org/langual_thesaurus.asp?termid=A0744" ;
                    rdfs:isDefinedBy "http://purl.obolibrary.org/obo/FOODON_03400744" ;
