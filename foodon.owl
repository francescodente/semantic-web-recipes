                    rdfs:subClassOf obo:FOODON_03400661 ;
                    obo:IAO_0000114 obo:IAO_0000428 ;
                    obo:IAO_0000115 "Miscellaneous derived edible products include various intermediate products in the manufacture of edible food products. Some of these are used for further processing and not consumed as food or feed as such."@en ;
                    obo:IAO_0000412 "http://langual.org" ;
