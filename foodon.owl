                    rdfs:label "amphibian and reptile"@en ;
                    skos:definition "living things that are cold-blooded and have backbones" ;
                    skos:scopeNote "used for ingredient classification" .


###  http://purl.obolibrary.org/obo/FOODON_03400658
obo:FOODON_03400658 rdf:type owl:Class ;
                    rdfs:subClassOf obo:FOODON_03400645 ;
                    obo:IAO_0000114 obo:IAO_0000428 ;
                    obo:IAO_0000412 "http://langual.org" ;
                    dct:source "SoftSchool.com, \"Amphibians vs. Reptiles.\" [Online]. Available: http://www.softschools.com/difference/amphibians_vs_reptiles/42/. [Accessed Nov.16, 2018]" ;
                    oboInOwl:hasDbXref "http://www.langual.org/langual_thesaurus.asp?termid=A0658" ;
