                    rdfs:subClassOf obo:FOODON_03400661 ;
                    obo:IAO_0000114 obo:IAO_0000428 ;
                    obo:IAO_0000115 "Cereal grain milling fractions includes milling fractions of cereal grains at the final stage of milling and separation in the fractions. The group also includes the processed brans, as prepared for direct consumption."@en ;
                    obo:IAO_0000412 "http://langual.org" ;
