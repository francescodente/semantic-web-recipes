    <!-- http://www.semanticweb.org/it/unibo/semantic-web/recipes#Pancake_Cook -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/it/unibo/semantic-web/recipes#Pancake_Cook">
        <rdf:type rdf:resource="http://www.semanticweb.org/it/unibo/semantic-web/recipes#Step"/>
        <recipes:hasDescription>Cook until surface of pancakes have some bubbles and a few have burst, 1 to 2 minutes. Flip carefully with a thin spatula, and cook until browned on the underside, 1 to 2 minutes more. Transfer to a baking sheet or platter; cover loosely with aluminum foil, and keep warm in oven. Continue with more oil and remaining batter. (You&apos;ll have 12 to 15 pancakes.) Serve warm, with desired toppings.</recipes:hasDescription>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/it/unibo/semantic-web/recipes#Pancake_Mix -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/it/unibo/semantic-web/recipes#Pancake_Mix">
        <rdf:type rdf:resource="http://www.semanticweb.org/it/unibo/semantic-web/recipes#Step"/>
        <recipes:hasNext rdf:resource="http://www.semanticweb.org/it/unibo/semantic-web/recipes#Pancake_Mix2"/>
        <recipes:hasDescription>In a small bowl, whisk together flour, sugar, baking powder, and salt; set aside.</recipes:hasDescription>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/it/unibo/semantic-web/recipes#Pancake_Mix2 -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/it/unibo/semantic-web/recipes#Pancake_Mix2">
